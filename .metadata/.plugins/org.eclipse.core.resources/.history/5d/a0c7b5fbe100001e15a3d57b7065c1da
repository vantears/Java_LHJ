package day4.practice;
import java.util.Scanner

public class Ex1 {

	public static void main(String[] args) {
		
		int age;
		Scanner sc = new Scanner(System.in);
		age = sc.nextInt();
		if (age >= 20) {
		System.out.println("Adult");
	} 
		 else {
		System.out.println("Minor");
  }
		sc.close();
 }
}