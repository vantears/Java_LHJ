package day12.practice;

public class VocabularyNote {
	
	private Word wordlist[];
	private int wordCount;
	
	public VocabularyNote(String word, String meaning) {
		this.wordlist = new Word[5];
		this.wordlist[0] = new Word(word, meaning);
		wordCount++;
	}
	
	public void print() {
		System.out.println("===============");
		for(int i = 0; i < wordCount; i++) {
			wordlist[i].print();
		}
	}
}
