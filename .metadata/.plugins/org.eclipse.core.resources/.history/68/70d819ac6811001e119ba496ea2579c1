package day18.practice;

public class StringIndexOfEx {

	public static void main(String[] args) {
		String str = "Hello world";
		int count = 0;
		for(int i = 0; i < str.length(); i++) {
			if(str.indexOf("l", i) != -1){
				i = str.indexOf("l", i);
				count++;
			} else {
				break;
			}
		}
		System.out.println(count);

	}

}
