package day11.practice;
import java.util.Scanner;
public class ArrayRandomEx {
	
	private static boolean randomChecker(int n[], int index) {
		for(int i = 0; i < n.length; i++) {
			if(n[i] == index) {
				return true;
			}
		}
		return false;
	}

	public static void main(String[] args) {
		int index, length, max;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the length of array : ");
		length = sc.nextInt();
		int[] randomArray = new int[length];
		random ran = new random();
		randomChecker ch = new randomChecker();
		max = ran.getMax();
		if(max < length) {
			ran.setMax(length);
		}
		for(int i = 0; i < length; i++) {
			do {
				index = ran.randomer();
			} while(ch.Checker(randomArray, index));
			randomArray[i] = index;
			System.out.println(randomArray[i]);
		}
		sc.close();
	}

}

class random {
	private int random;
	private int min = 1;
	private int max = 9;
	public int randomer() {
		random = (int)(Math.random() * (max - min + 1) + min);
		return random;
	}
	public int getMax() {
		return max;
	}
	public void setMax(int max) {
		this.max = max;
	}	
}

class randomChecker {
	public boolean Checker(int n[], int index) {
		for(int i = 0; i < n.length; i++) {
			if(n[i] == index) {
				return true;
			}
		}
		return false;
	}
}
